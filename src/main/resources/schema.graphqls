type Continent {
  id: ID,
  label: String,
  countries: [Country]
}

type Language {
  id: ID,
  name: String,
  nativeName: String
}

type Country {
  id: ID!,
  name: String,
  nativeName: String,
  phone: String,
  continent: Continent,
  currencies: String,
  languages: [Language]
}

type Query {
  countries: [Country]
  continents: [Continent]
}